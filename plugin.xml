<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="RoboCopy.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Sample Action"
               icon="icons/robocopy_2.gif"
               class="robocopy.actions.Action"
               tooltip="Robocopy"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="robocopy.actions.Action">
         </action>
      </actionSet>
   </extension>

</plugin>
